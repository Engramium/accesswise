{
  "name": "accesswise",
  "version": "1.0.1",
  "type": "module",
  "description": "Power to restrict your website and it's content via Age Gate, Force Login, Disable Right click",
  "scripts": {
    "dev": "vite",
    "build:assets": "vite build",
    "build": "composer build && vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "wp-cli-pot": "wp i18n make-pot . i18n/accesswise.pot",
    "zip": "node ./zip.build.cjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/engramium/accesswise"
  },
  "keywords": [
    "wordpress-plugin"
  ],
  "author": "Sayedul Sayem <sayedulsayem@gmail.com>",
  "license": "GPL-3.0-or-later",
  "engines": {
    "npm": ">=9.8.1",
    "node": ">=19.4.0"
  },
  "browserslist": [
    "last 2 version",
    "> .5%"
  ],
  "dependencies": {
    "element-plus": "^2.5.5",
    "vue": "^3.3.11",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.2",
    "archiver": "^7.0.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.55.0",
    "postcss": "^8.4.35",
    "sass": "^1.71.0",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.8"
  }
